{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:app:default",
    "core:event:default",
    "core:window:default",
    "core:webview:default",
    "core:path:default",
    "opener:default",
    "log:default",
    "log:allow-log",
    "shell:allow-open",
    "stronghold:default",
    "sql:default",
    "sql:allow-load",
    "sql:allow-select",
    "sql:allow-execute",
    "sql:allow-close"
  ]
}
